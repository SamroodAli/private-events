<h1>Event: <%= @event.id %></h1>
<h2>by:<%= @event.creator.name%></h2>
<br>
<p>
  <%= @event.description %>
</p>
<br>
<p>
  Time:  <%= @event.date %>
</p>
<h3>Attendees</h3>
<ul>
  <% @event.attendees.each do |attendee| %>
    <li>
      <% if attendee == current_user %>
        <%= "Me" %>
      <% else %>
        <%= attendee.name %>
      <% end %>
    </li>
  <% end %>
</ul>
